<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dkadtech.hawaiikai.mapper.DeviceInfoMapper">
    <select id="getDeviceInfoList" resultType="com.dkadtech.hawaiikai.domain.DeviceInfo">
        select
            device_info_id,
            device_name,
            device_auth_key,
            device_address,
            user_info_id
        from
            tb_device_info
    </select>
</mapper>