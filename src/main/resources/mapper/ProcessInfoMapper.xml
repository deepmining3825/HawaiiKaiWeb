<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dkadtech.hawaiikai.mapper.ProcessInfoMapper">
    <select id="getProcessInfoList" resultType="com.dkadtech.hawaiikai.domain.ProcessInfo">
        select
            process_info_id,
            process_num,
            process_status,
            device_info_id
        from
            tb_process_info
    </select>
</mapper>