<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dkadtech.hawaiikai.mapper.UserInfoMapper">
    <select id="getUserInfoList" resultType="com.dkadtech.hawaiikai.domain.UserInfo">
        select
            user_info_id,
            user_id,
            user_pw,
            push_alarm_address
        from
            tb_user_info
    </select>
</mapper>